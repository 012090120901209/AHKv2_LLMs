# AHKv2_LLMs Environment Variables
# Copy this file to .env and fill in your values

# GitHub Personal Access Token (optional)
# Used for higher API rate limits when updating the feed
# Create a token at: https://github.com/settings/tokens
# Required scopes: public_repo (read-only access)
GITHUB_TOKEN=ghp_your_token_here

# Feed Configuration
# Number of repositories to fetch and cache
FEED_LIMIT=100

# Update interval in hours (for scheduled workflows)
UPDATE_INTERVAL=6
